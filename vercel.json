{
  "version": 2,
  "functions": {
    "api/edge-function.js": {
      "runtime": "edge"
    }
  },
  "builds": [
    {
      "src": "index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    { "src": "/api/edge-function", "dest": "api/edge-function.js" },
    { "src": "/api/(.*)", "dest": "index.js" },
    { "src": "/api-docs", "dest": "index.js" },
    { "src": "/docs", "dest": "index.js" },
    { "src": "/grup", "dest": "index.js" },
    { "src": "/telegram", "dest": "index.js" },
    { "src": "/discord", "dest": "index.js" },
    { "src": "/", "dest": "index.js" },
    { "src": "/(.*)", "dest": "index.js" }
  ]
}